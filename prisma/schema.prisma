// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Booking {
  email String @unique
  name String
  date String
  time String
  message String
  approved String
}

model Busy {
  id Int @id @default(autoincrement())
  busy Boolean @default(false)
}

model User {
  email String @unique
  name String
  bookings Int
  sessions Int
}